include ../footer/footer
include ../counter/counter
mixin section-9
    section.section.section-9
        .section-9__wrap-red
            .container
                h1.section-9__title PartnersRegister for a conference
                p.section-9__subtitle and get ultimate guide to crypto investment
                .section-9__arrow
                    svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 31.479 31.479" style="enable-background:new 0 0 31.479 31.479;" xml:space="preserve")
                        path(style="fill:#FFF;" d="M26.485,21.206c0.429-0.444,0.429-1.143,0-1.587c-0.444-0.429-1.159-0.429-1.587,0l-8.047,8.047V1.111C16.851,0.492,16.359,0,15.74,0c-0.619,0-1.127,0.492-1.127,1.111v26.555l-8.031-8.047c-0.444-0.429-1.143-0.429-1.587,0c-0.429,0.444-0.429,1.143,0,1.587l9.952,9.952c0.429,0.429,1.143,0.429,1.587,0L26.485,21.206z")
                .section-9__btn
                    .btn Get free access
                .section-9__counter
                  +counter
            .section-9__wrap-dark
                .container
                   p.section-9__text Join to the community who runs the market:
                   .section-9__socials
                       - var icons = ['facebook', 'telegram', 'youtube', 'twitter']
                        each item in icons
                           .section-9__social
                               img(src="../images/" + item + ".svg")
