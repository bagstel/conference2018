mixin footer
    footer.footer
        div.social
            div.social__title Join to the community who runs the market:
            ul.social__list
                - var icons = ['facebook', 'telegram', 'youtube', 'twitter']
                each item in icons
                    li.social__item
                        img(src="../images/" + item + ".svg")

        div.copyright
            div.copyright__innet Â© Copyright 2018. All Rights Reserved